[sub_resource type="GDScript" id="GDScript_elgsa"]
resource_name = "weeeee"
script/source = "extends Node

var peer : ENetMultiplayerPeer
var address : String = \"localhost\"
var port : int = 12345

var canFunction : bool = true

var localPlayer : CharacterBody3D

@export var playerSpawner : MultiplayerSpawner
var charNode : PackedScene = preload(\"res://resources/scenes/character.tscn\")

func createPlayer(peerPressure : int) -> NetworkRigidBody3D:
	var newPlayerNode : NetworkRigidBody3D = charNode.instantiate()
	newPlayerNode.set(\"name\",str(peerPressure))
	newPlayerNode.set(\"peerId\",peerPressure)
	print(\"PEER ID:\",peerPressure)
	return newPlayerNode

func _ready() -> void:
	multiplayer.multiplayer_peer = null
	playerSpawner.spawn_function = createPlayer

func _on_join_pressed() -> void:
	if not canFunction: return
	canFunction = false
	peer = ENetMultiplayerPeer.new()
	peer.create_client(address,port)
	multiplayer.multiplayer_peer = peer

func _on_host_pressed() -> void:
	if not canFunction: return
	canFunction = false
	peer = ENetMultiplayerPeer.new()
	peer.create_server(port)
	multiplayer.multiplayer_peer = peer
	peer.peer_connected.connect(on_peer_connected)
	#on_peer_connected(1)

func on_peer_connected(id : int) -> void:
	if multiplayer.has_multiplayer_peer():
		playerSpawner.spawn(id)
"

